extends ../../layouts/dashboard-unauthenticated.pug

block content
  +container-fluid   
    +row   
      +col({ lg: 12 })
        div(class='error-section')
          div(class='error-icon') 
            +pe('mouse') 
          div(class='error-content')
            div(class='error-title') Error 404
            div(class='error-subtitle') Oops! Looks like the page you tried to reach doesn't exist.
            a(class='btn -dark -wide' href=server.url + "/") Back to Homepage
