extends ../../layouts/dashboard.pug

block content
  +container-fluid
    +row 
      +col({ lg: 12 })
        +page-heading(server, page)
    
    +row
      +col({ md: 6 })
        +panel(class='-dark')
          +panel-heading 
            h4 Line Chart
            span A showcase of Volta's Morris.js chart integration
          +panel-body
            div(class='chart' id='morris-chart-line' height="220")
      +col({ md: 6 })
        +panel.-dark
          +panel-heading 
            h4 Bar Chart
            span A showcase of Volta's Morris.js chart integration
          +panel-body
            div(class='chart' id='morris-chart-bar' height="220")
        
    +row
      +col({ md: 6 })
        +panel(class='-dark')
          +panel-heading 
            h4 Donut Chart
            span A showcase of Volta's Morris.js chart integration
          +panel-body
            div(class='chart' id='morris-chart-donut' height="220")
      +col({ md: 6 })
        +panel.-dark
          +panel-heading 
            h4 Area Chart
            span A showcase of Volta's Morris.js chart integration
          +panel-body
            div(class='chart' id='morris-chart-area' height="220")
