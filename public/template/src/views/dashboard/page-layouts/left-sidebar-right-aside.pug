extends ../../layouts/dashboard.pug

block layout
  +navbar(server, 'dark')      

  +sidebar-main({ server: server, pages: pages[1], theme: 'dark' })(class="-collapsible") 
  +sidebar-secondary(server, page['view'], 'dark') 
  
  +content('dark')(class='-collapsible') 
    block content
      +aside(class='-dark -right -with-left-sidebar -collapse-with-sidebar -collapsible')
        div(class='aside-content') Page Aside 
          
      +beside(class='-right-aside -collapse-with-sidebar -collapsible')
        +container-fluid
          +row 
            +col({ lg: 12 })
              +page-heading(server, page)
      
